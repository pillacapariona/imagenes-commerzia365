      whatsappButton.style.borderRadius = "20px";
      whatsappButton.style.fontWeight = "bold";
      whatsappButton.style.textDecoration = "none";
      whatsappButton.style.fontSize = "14px";
      whatsappButton.style.boxShadow = "0px 4px 10px rgba(0, 0, 0, 0.1)";

      // Mensaje según el tipo de página
      var whatsappNumber = "51981515659"; // Número de WhatsApp en formato internacional, sin signos (+) ni espacios
      var message;

